<p id="notice"><%= notice %></p>

<h1>Stss Trades</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stss_trades.each do |stss_trade| %>
      <tr>
        <td><%= link_to 'Show', stss_trade %></td>
        <td><%= link_to 'Edit', edit_stss_trade_path(stss_trade) %></td>
        <td><%= link_to 'Destroy', stss_trade, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stss Trade', new_stss_trade_path %>
